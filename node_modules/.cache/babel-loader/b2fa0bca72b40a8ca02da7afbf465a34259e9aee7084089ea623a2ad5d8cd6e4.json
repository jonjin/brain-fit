{"ast":null,"code":"export function calculateTaskScore(_ref){let{accuracy,speed,concentration}=_ref;const weighted=accuracy*0.6+speed*0.2+concentration*0.2;return Math.round(Math.min(100,Math.max(0,weighted)));}export function calculateSessionSummary(scores){if(!scores.length){return{averageScore:0,bestScore:0,sessionsCompleted:0};}const bestScore=Math.max(...scores);const averageScore=Math.round(scores.reduce((acc,value)=>acc+value,0)/scores.length);return{averageScore,bestScore,sessionsCompleted:scores.length};}","map":{"version":3,"names":["calculateTaskScore","_ref","accuracy","speed","concentration","weighted","Math","round","min","max","calculateSessionSummary","scores","length","averageScore","bestScore","sessionsCompleted","reduce","acc","value"],"sources":["/Users/apple-dev/Desktop/test3/brain-fit/src/utils/scoreCalculator.ts"],"sourcesContent":["interface TaskScoreInput {\n  accuracy: number;\n  speed: number;\n  concentration: number;\n}\n\nexport function calculateTaskScore({ accuracy, speed, concentration }: TaskScoreInput): number {\n  const weighted = accuracy * 0.6 + speed * 0.2 + concentration * 0.2;\n  return Math.round(Math.min(100, Math.max(0, weighted)));\n}\n\nexport function calculateSessionSummary(scores: number[]) {\n  if (!scores.length) {\n    return {\n      averageScore: 0,\n      bestScore: 0,\n      sessionsCompleted: 0,\n    };\n  }\n\n  const bestScore = Math.max(...scores);\n  const averageScore = Math.round(scores.reduce((acc, value) => acc + value, 0) / scores.length);\n\n  return {\n    averageScore,\n    bestScore,\n    sessionsCompleted: scores.length,\n  };\n}\n"],"mappings":"AAMA,MAAO,SAAS,CAAAA,kBAAkBA,CAAAC,IAAA,CAA6D,IAA5D,CAAEC,QAAQ,CAAEC,KAAK,CAAEC,aAA8B,CAAC,CAAAH,IAAA,CACnF,KAAM,CAAAI,QAAQ,CAAGH,QAAQ,CAAG,GAAG,CAAGC,KAAK,CAAG,GAAG,CAAGC,aAAa,CAAG,GAAG,CACnE,MAAO,CAAAE,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,GAAG,CAAC,GAAG,CAAEF,IAAI,CAACG,GAAG,CAAC,CAAC,CAAEJ,QAAQ,CAAC,CAAC,CAAC,CACzD,CAEA,MAAO,SAAS,CAAAK,uBAAuBA,CAACC,MAAgB,